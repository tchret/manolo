{
  "name": "manolo",
  "version": "1.0.0",
  "description": "Manolo's website",
  "author": "tchret",
  "private": true,
  "scripts": {
    "dev": "nuxt",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "precommit": "npm run lint",
    "deploy": "rm -rf dist && DEPLOY_ENV=GH_PAGES nuxt generate && push-dir --dir=dist --branch=gh-pages --cleanup --verbose"
  },
  "dependencies": {
    "nuxt": "^1.0.0",
    "nuxt-sass-resources-loader": "^2.0.5",
    "push-dir": "^0.4.1"
  },
  "devDependencies": {
    "babel-eslint": "^8.2.1",
    "cross-env": "^5.0.1",
    "eslint": "^4.15.0",
    "eslint-loader": "^1.7.1",
    "eslint-plugin-vue": "^4.0.0"
  }
}
